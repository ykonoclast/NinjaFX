<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:controller="org.duckdns.spacedock.ninjafx.StuffController" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
	<VBox fx:id="VBoxMain" alignment="TOP_CENTER" AnchorPane.topAnchor="5" AnchorPane.leftAnchor="5">
	    <children>
		<Label fx:id="TitleLbl" alignment="TOP_LEFT" text="ListView Example">
		    <font>
			<Font name="System Bold" size="18.0" />
		    </font>
		</Label>
		<ListView fx:id="listBoxMain"/>
		<Label id="TextLbl" fx:id="LblAddText" text="Enter Text" />
		<TextField fx:id="txtAddItem"/>
		<HBox id="HBox" fx:id="HBox4Btns" alignment="CENTER" spacing="5.0">
		    <children>
			<Button fx:id="BtnAdd" mnemonicParsing="false" onAction="#addAction" text="Add" />
			<Button fx:id="BtnDelete" mnemonicParsing="false" onAction="#deleteAction" text="Delete" />
		    </children>
		</HBox>
	    </children>
	</VBox>
    </children>
</AnchorPane>