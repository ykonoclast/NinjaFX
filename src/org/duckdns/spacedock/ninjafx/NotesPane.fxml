<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox fx:controller="org.duckdns.spacedock.ninjafx.NotesController" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:id="fx_aVBoxMain">
    <children>
	<Label fx:id="fx_aTitleLbl" alignment="TOP_LEFT" text="I must remember...">
	    <font>
		<Font name="System Bold" size="18.0" />
	    </font>
	</Label>
	<ListView fx:id="fx_listBoxMain"/>
	<TextField fx:id="fx_txtAddItem"/>
	<HBox fx:id="fx_HBox4Btns" alignment="CENTER" spacing="5.0">
	    <children>
		<Button fx:id="fx_BtnAdd" mnemonicParsing="false" onAction="#addAction" text="Add" />
		<Button fx:id="fx_BtnDelete" mnemonicParsing="false" onAction="#deleteAction" text="Delete" />
	    </children>
	</HBox>
    </children>
</VBox>
